unit About;

interface

uses Windows, SysUtils, Classes, Graphics, Forms, Controls, StdCtrls,
  Buttons, ExtCtrls, RbDrawCore, RbButton;

type
  TAboutBox = class(TForm)
    Copyright1: TLabel;
    Version: TLabel;
    ProductName: TLabel;
    Email: TLabel;
    Skull: TImage;
    ICQUIN: TLabel;
    Copyright2: TLabel;
    procedure SkullClick(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  AboutBox: TAboutBox;

implementation

{$R *.dfm}

procedure TAboutBox.SkullClick(Sender: TObject);
begin
  AboutBox.Close;
end;

end.
